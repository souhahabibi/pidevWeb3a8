<!DOCTYPE html>

<title>Reviewmeal</title>

{% block body %}
    <h1>Reviewmeal</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ reviewmeal.id }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ reviewmeal.rate }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ reviewmeal.comment }}</td>
            </tr>
            <tr>
                <th>CreatedDate</th>
                <td>{{ reviewmeal.createdDate ? reviewmeal.createdDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ reviewmeal.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_reviewmeal_index') }}">back to list</a>

    <a href="{{ path('app_reviewmeal_edit', {'id': reviewmeal.id}) }}">edit</a>

    {{ include('reviewmeal/_delete_form.html.twig') }}
{% endblock %}
